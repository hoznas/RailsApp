<h1>Messages#show</h1>
<p><%= @msg %></p>

<table class="table">
  <tr>
    <th>Id</th>
    <td></td>
  </tr>

  <tr>
    <th>Person</th>
    <td><%= @message.person_id %></td>
  </tr>

  <tr>
    <th>Title</th>
